
<div class="container-fluid" style="margin-top:50px;">
	<form class="form-horizontal" role="form">
	  <div class="form-group">
	    <label for="oldpass" class="col-sm-3 control-label">请输入密码</label>
	    <div class="col-sm-9">
	      <input type="password" class="form-control" style="width:315px; border-radius: 5px" id="J_oldPass" ><span  style="display:none;color:red;"></span>
	    </div>
	  </div>
	  <div class="form-group">
	    <label for="newpass" class="col-sm-3 control-label">请输入新密码</label>
	    <div class="col-sm-9">
	      <input type="password" class="form-control" style="width:315px; border-radius: 5px" id="J_newPass" ><span  style="display:none;color:red;"></span>
	    </div>
	  </div>
	  <div class="form-group">
	    <label for="newpassAgain" class="col-sm-3 control-label">请再输入一次</label>
	    <div class="col-sm-9">
	      <input type="password" class="form-control" style="width:315px;border-radius: 5px" id="J_repeatNewPass" ><span style="display:none;color:red;"></span>
	    </div>
	  </div>
	  <div class="form-group text-center">
	 	 <button type="button" class="btn" id="J_update" style="text-align:center;border-radius: 5px;background-color:#00b38a;color:#fff">更新密码</button>
	  </div>
	</form>
</div>

<script type="text/javascript" src="../scripts/p_password_reset.js"></script>
