<meta charset="UTF-8">
<link rel="stylesheet" type="text/css" href="../styles/p_user_list.css">
<link rel="stylesheet" type="text/css" href="../styles/p_insitution_list.css">

<div class="container-fluid">
    <h2 class="title">反馈管理</h2>
	<form class="form-inline" id="QueryUserForm">
		<div class="form-group">
			<label for="feedbackStatus">状态</label> 
			<select id="feedbackStatus" class="form-control">
			<option value='0'>所有</option>
			<option value='1'>未处理</option>
			<option value='2'>已处理</option>
			</select>
		</div>
		<div class="form-group">
			<label for="username">手机</label> <input type="text"
				class="form-control" name="mobileF" id="mobileF">
		</div>
		<a class="btn btn-default bt-color btn-sm" role="button" id="queryFeedbackList">
		    <span  class="gicon">搜索</span>
	    </a>
	</form>
	
	 <div id="feedback_list"></div>
          <nav>
    <div class="pagin-btm clearFix innerWord">
        <div class="pagin" id="page"></div>
    </div>
    </nav>  
	
	<nav>
        <div class="pagination pull-right radiusIcon" style="margin:10px 0px;" id="page">
        </div>
    </nav>
         <script id="v_empty_list" type="text/html">
            <div class="codeContent field min-height" id="project-interface">
                <div class="cooperate-status">
                    <p>当前列表没有数据！</p>
                </div>
            </div>
        </script>
       <script id="t:feedback_list" type="text/html">
            <table class="table table-striped table-bordered" style="margin-bottom: 0px">
                <tbody>
                <tr>
                <td>手机号</td>
				<td>反馈信息</td>
				<td>联系方式</td>
				<td>反馈时间</td>
				<td>状态</td>
				<td>操作</td>
                </tr>
                {{each list as l}}
                <tr>  
                   <td>{{l.contact}}</td>
                   <td>{{stripStr(l.info)}}</td>
                   <td>{{l.contact}}</td>  
                    <td>{{l.createTimeStr}}</td>
                     <td>  
                        {{if l.status==1}}未处理
                        {{else}}已处理{{/if}}
                   </td>
                    <td>
                        {{if l.status==1}}
                        <a href="#" class="J_update" data-id="{{l.id}}" >处理</a>
                        {{/if}}
                    </td>
                </tr>
                {{/each}}  
                </tbody>
            </table>
        </script>
    <script type="text/javascript" src="../modules/dateFormat/dateFormat.js"></script>
          <script type="text/javascript" src="../scripts/p_feedback_list.js"></script>
</div>
